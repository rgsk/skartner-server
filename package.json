{
  "name": "skartner-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=src ts-node src/server.ts",
    "serve": "NODE_PATH=src ts-node-dev --transpile-only --no-notify src/server.ts",
    "dev": "sh ./bin.local/start.sh",
    "build": "tsc",
    "kill:process": "kill -9 $(lsof -t -i:8001)",
    "generate": "NODE_PATH=src ts-node --transpile-only src/schema.ts",
    "propagate:schema": "cp schema.graphql ../skartner-app/lib/schema.graphql",
    "watch:schema": "nodemon -w schema.graphql --exec \"yarn propagate:schema\"",
    "test": "jest",
    "init:prisma": "prisma generate",
    "studio": "BROWSER=none prisma studio",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "migrate:force-reset": "prisma migrate reset --force",
    "husky-prepare": "husky install",
    "format": "prettier --write .",
    "seeders": "NODE_PATH=src ts-node src/seeders/index.ts"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@apollo/utils.keyvadapter": "^3.0.0",
    "@aws-sdk/client-s3": "^3.458.0",
    "@aws-sdk/lib-storage": "^3.458.0",
    "@bull-monitor/express": "^5.1.0",
    "@graphql-tools/schema": "^10.0.0",
    "@keyv/redis": "^2.6.1",
    "@prisma/client": "^4.1.0",
    "axios": "^1.6.2",
    "bull": "^4.10.4",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "graphql": "^16.5.0",
    "graphql-middleware": "^6.1.35",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-shield": "^7.6.5",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.13.1",
    "ioredis": "^5.3.2",
    "keyv": "^4.5.2",
    "nexus": "^1.3.0",
    "openai": "^4.20.0",
    "ws": "^8.13.0",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^28.1.6",
    "@types/sqlite3": "^3.1.8",
    "@types/ws": "^8.5.4",
    "get-port": "^6.1.2",
    "graphql-request": "^4.3.0",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.1.0",
    "sqlite3": "^5.0.9",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
